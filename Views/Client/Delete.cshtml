@model RealEstateMvcApp.Models.Dtos.ClientDto
<div><i>Are you sure you want to delete this Client?</i></div>
<div>
    <h4>@Model.Id</h4>
    <h4>@Model.Name</h4>
    <h4>@Model.Email</h4>
    <h4>@Model.PhoneNumber</h4>
    <h4>@Model.Address</h4>
</div>
<form asp-action="Delete">
    <input type="hidden" value="@Model.Id"/>
    <input class="btn btn-danger" type="submit" value="Delete"/>
    <li>
            <a asp-controller="Client" asp-action="List">No</a>
        </li>
</form>